# === Runtime Environment ===
ENV=DEV
DEBUG=1

# === OpenAI ===
OPENAI_MODEL=gpt-3.5-turbo
OPENAI_API_KEY=sk-proj-...
OPENAI_PROJECT_ID=proj_...

# === LLM Agent Config ===
LLM_MAX_TOKENS=500
LLM_TEMPERATURE=0.0

# === Network Settings ===
MAX_CONCURRENT_REQUESTS=3
RETRY_ATTEMPTS=1

# === Concurrency Settings ===
FETCH_CONCURRENCY=10
LLM_CONCURRENCY=10

# === Screenshot Settings ===
SCREENSHOT_ENABLED=false
SCREENSHOT_DIR=screenshots

# === Logging Settings ===
LOG_LEVEL=INFO
LOG_DIR=logs
LOG_MAX_BYTES=1000000
LOG_BACKUP_COUNT=5
LOG_FORMAT=plain
